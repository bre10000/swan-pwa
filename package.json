{
  "name": "swan-pwa",
  "description": "Progressive web application for SWAN",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev --port 5555",
    "dev:sass": "npm run css-watch & sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --basepath swan-pwa --legacy",
    "export-normal": "sapper export ./build",
    "export-github": "sapper export  --basepath ./build --legacy",
    "start": "node __sapper__/build",
    "css-build": "node-sass --omit-source-map-url --output-style compressed src/styles/global.scss static/css/global.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run export && node ./scripts/gh-pages.js"
  },
  "dependencies": {
    "@beyonk/svelte-carousel": "^2.8.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "compression": "^1.7.1",
    "express": "^4.18.1",
    "frappe-charts": "^1.6.2",
    "gh-pages": "^3.2.3",
    "helmet": "^5.1.0",
    "http-proxy-middleware": "^1.3.1",
    "idb-keyval": "^6.1.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "node-fetch": "^2.6.6",
    "polka": "next",
    "qs": "^6.10.3",
    "sapper-page-loading-bar": "^1.0.0",
    "sirv": "^1.0.19",
    "svelte-awesome": "^2.4.2",
    "svelte-i18n": "^3.3.13",
    "trouter": "^3.2.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.16.3",
    "@beyonk/svelte-social-auth": "^2.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-url": "^5.0.0",
    "@zerodevx/svelte-toast": "^0.7.1",
    "bulma": "^0.9.3",
    "cookie-universal": "^2.1.5",
    "date-picker-svelte": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.60.2",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.44.2",
    "svelte-forms": "^2.2.1",
    "svelte-select": "^4.4.7"
  }
}
